{
    "name": "text-completion-api",
    "description": "A Text Completion API that predicts the next word in a given text.",
    "version": "1.0.2",
    "packageManager": "yarn@3.2.4",
    "scripts": {
        "dev": "concurrently \"nodemon\" \"nodemon -x tsoa spec-and-routes\"",
        "tsoa:gen": "tsoa routes && tsoa swagger",
        "build": "tsoa spec-and-routes && tsc",
        "init-db": "tsc &&  node build/src/perform/initDatabase.perf.js",
        "start": "node build/src/server.js",
        "test": "tsc && node build/src/tests/index.js"
    },
    "repository": "https://github.com/chrisyao4700/text-completion-api.git",
    "author": "chrisyao4700 <chrisyao900908@gmail.com>",
    "license": "MIT",
    "devDependencies": {
        "@types/cls-hooked": "^4.3.3",
        "@types/cors": "^2.8.12",
        "@types/morgan": "^1.9.3",
        "@types/swagger-ui-express": "^4.1.3",
        "concurrently": "^7.4.0",
        "nodemon": "^2.0.20",
        "prettier": "2.8.3",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.5"
    },
    "dependencies": {
        "@google-cloud/speech": "^5.3.0",
        "@google-cloud/translate": "^7.1.0",
        "@types/node": "^18.8.5",
        "@types/xml2js": "^0.4.11",
        "aws-sdk": "^2.1313.0",
        "axios": "^1.3.2",
        "blob": "^0.1.0",
        "cls-hooked": "^4.2.2",
        "cors": "^2.8.5",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "express-status-monitor": "^1.3.4",
        "form-data": "^4.0.0",
        "formdata-node": "^5.0.0",
        "grammarify": "^2.1.0",
        "http-auth": "^4.2.0",
        "jsonwebtoken": "^9.0.0",
        "microsoft-cognitiveservices-speech-sdk": "^1.25.1",
        "morgan": "^1.10.0",
        "node-fetch": "^3.3.0",
        "openai": "^3.1.0",
        "path": "^0.12.7",
        "rhea": "^3.0.2",
        "sequelize": "^6.28.0",
        "sha1": "^1.1.1",
        "sqlite3": "^5.1.4",
        "swagger-ui-express": "^4.5.0",
        "tsoa": "^4.1.3",
        "uuid": "^9.0.0",
        "winston": "^3.8.2",
        "xml2js": "^0.4.23"
    }
